[
    {
        "key": "ctrl+f5",
        "command": "workbench.action.debug.start",
        "when": "inDebugMode"
    },
    {
        "key": "f5",
        "command": "workbench.action.debug.run",
        "when": "!inDebugMode"
    },
    {
        "key": "shift+tab",
        "command": "workbench.action.focusSecondEditorGroup",
        "when": "activeEditorGroupIndex == 1"
    },
    {
        "key": "shift+tab",
        "command": "workbench.action.focusFirstEditorGroup",
        "when": "activeEditorGroupIndex == 2"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.quickSwitchWindow"
    },
    {
        "key": "f12",
        "command": "python.execInTerminal"
    },
    {
        "key": "ctrl+shift+a",
        "command": "jupyter.collapseallcells"
    },
    {
        "key": "ctrl+shift+z",
        "command": "jupyter.runcell"
    },
    {
        "key": "ctrl+shift+x",
        "command": "jupyter.removeallcells"
    },
    {
        "key": "ctrl+l",
        "command": "expandLineSelection"
    },
    {
        "key": "ctrl+shift+=",
        "command": "search.action.clearSearchResults"
    },
    {
        "key": "alt+up",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+down",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+[",
        "command": "editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+shift+]",
        "command": "editor.unfoldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+[",
        "command": "editor.fold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+]",
        "command": "editor.unfold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+shift+right",
        "command": "-cursorWordAccessibilityRightSelect",
        "when": "accessibilityModeEnabled && textInputFocus"
    },
    {
        "key": "ctrl+shift+right",
        "command": "-cursorWordEndRightSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+left",
        "command": "-cursorWordStartLeftSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+left",
        "command": "-cursorWordAccessibilityLeftSelect",
        "when": "accessibilityModeEnabled && textInputFocus"
    },
    {
        "key": "shift+alt+left",
        "command": "workbench.action.nextPanelView"
    },
    {
        "key": "alt+right",
        "command": "-workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+right",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "alt+left",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+alt+k",
        "command": "jupyter.restartkernel"
    },
    {
        "key": "ctrl+alt+x",
        "command": "-workbench.view.extension.latex",
        "when": "!config.latex-workshop.bind.altKeymap.enabled"
    },
    {
        "key": "ctrl+alt+r",
        "command": "jupyter.runFileInteractive"
    },
    {
        "key": "ctrl+shift+right",
        "command": "inlineBookmarks.jumpToNext"
    },
    {
        "key": "ctrl+shift+left",
        "command": "inlineBookmarks.jumpToPrevious"
    },
    {
        "key": "ctrl+alt+0",
        "command": "sidebarMarkdownNotes.webview.focus"
    },
    {
        "key": "ctrl+alt+z",
        "command": "insertDateString.insertDate"
    },
    {
        "key": "ctrl+shift+e",
        "command": "workbench.files.action.focusOpenEditorsView",
        "when": "workbench.explorer.openEditorsView.active"
    },
    {
        "key": "ctrl+shift+f",
        "command": "workbench.explorer.fileView.focus"
    },
    {
        "key": "ctrl+shift+b",
        "command": "inlineBookmarksExplorer.focus"
    },
    {
        "key": "ctrl+alt+v",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "${1:var} = ${2:value}   | ${3:unit1, unit2} | ${0:descrip}"
        }
    },
    {
        "key": "ctrl+alt+e",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "${1:var} = ${2:relation}   | ${0:unit, altunit}"
        }
    },
    {
        "key": "ctrl+alt+a",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| attach | ${1:location} | ${0:filename or parameter}"
        }
    },
    {
        "key": "ctrl+alt+r",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| report | ${1:location} | ${0:filename or parameter}"
        }
    },
    {
        "key": "ctrl+alt+o",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| output | ${1:location} | ${0:filename or parameter}"
        }
    },
    {
        "key": "ctrl+alt+s",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| search | ${1:location} | ${0:filename or parameter}"
        }
    },
    {
        "key": "ctrl+alt+p",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| project | ${1:location} | ${0:filename or parameter}"
        }
    },
    {
        "key": "ctrl+alt+t",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| insert | table | ${1:file}.csv | ${2:width,align} | [${0:cols}]"
        }
    },
    {
        "key": "ctrl+alt+x",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| insert | text | ${1:file}.csv | ${2:width,align} | [${0:cols}]"
        }
    },
    {
        "key": "ctrl+k ctrl+x",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| eval | text | ${1:file}.csv | ${2:width,align} | [${0:cols}]"
        }
    },
    {
        "key": "ctrl+alt+i",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| insert | image | ${1:file}.csv | ${2:width,align} | [${0:cols}]"
        }
    },
    {
        "key": "ctrl+alt+m",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| insert | images | ${1:filename}, ${2:filename} | ${3:scale}, ${0:scale}"
        }
    },
    {
        "key": "ctrl+alt+f",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| format | ${0:trunc, trunc} | ${1:sub or nosub} "
        }
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| eval | data | ${1:file}.csv | $[0:data]"
        }
    },
    {
        "key": "ctrl+k ctrl+n",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| eval | function | ${1:file}.csv | $[0:data]"
        }
    },
    {
        "key": "ctrl+alt+g",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "[${0:tag}]_"
        }
    },
    {
        "key": "ctrl+alt+u",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "|| eval | values | ${0:file}.csv"
        }
    },
    {
        "key": "ctrl+alt+s",
        "command": "autoDocstring.generateDocstring",
        "when": "editorTextFocus"
    }
]